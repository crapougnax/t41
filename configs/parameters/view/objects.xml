				</parameter>
				<parameter id="post_ok" type="array">
					<bogus/>
				</parameter>
				<parameter id="post_nok" type="array">
					<bogus/>
				</parameter>
			</parameters>
		</object>
		
		<object id="\t41\View\ImageComponent" extends="\t41\View\ViewObject">
			<parameters>
				<parameter id="ratio">
					<type>float</type>
					<defaultvalue>1.0</defaultvalue>
