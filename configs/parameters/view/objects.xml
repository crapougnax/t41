				<parameter id="offset" type="integer">
					<defaultvalue>0</defaultvalue>
				</parameter>
				<parameter id="batch" type="integer">
					<defaultvalue>10</defaultvalue>
				</parameter>
				<parameter id="sorting" type="array">
					<bogus/>
				</parameter>
			</parameters>
		</object>
		
