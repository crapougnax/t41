				<parameter id="border">
					<type>integer</type>
					<defaultvalue>0</defaultvalue>
				</parameter>
			</parameters>
		</object>

		<object id="\t41\View\FormComponent\Element\ButtonElement" extends="\t41\View\ViewObject">
			<parameters>
				<parameter id="uri" type="object">
				<bogus/>
