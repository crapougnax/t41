				<parameter id="align" type="string">
					<bogus/>
				</parameter>
			</parameters>
		</object>
		
		<object id="\t41\View\ListComponent\Element\MetaElement" extends="\t41\View\ViewObject">
			<parameters>
				<parameter id="property" type="string">
