<?xml version="1.0" encoding="UTF-8"?>
<config>
	<decorators>
		<object id="\t41\View\SimpleComponent">
			<decorator id="WebDefault">
				<parameters>
					<parameter id="theme">
						<type>string</type>
					</parameter>
					<parameter id="color">
						<type>string</type>
					</parameter>
					<parameter id="open_default">
						<type>boolean</type>
						<defaultvalue>true</defaultvalue>
					</parameter>
				</parameters>
			</decorator>
			<decorator id="PdfDefault">
				<parameters>
					<parameter id="newpage">
						<type>string</type>
					</parameter>
				</parameters>
			</decorator>
		</object>
		<object id="\t41\View\FormComponent">
			<decorator id="WebDefault" extends="\t41\View\Decorator\AbtractWebDecorator">
				<parameters>
					<bogus/>
				</parameters>
			</decorator>
		</object>

		<object id="\t41\View\TableComponent">
			<decorator id="PdfDefault" extends="\t41\View\Decorator\AbtractPdfDecorator">
				<parameters>
					<parameter id="newpage">
						<type>string</type>
						<defaultvalue>no</defaultvalue>
						<values>
							<value id="no">no</value>
							<value id="before">before</value>
							<value id="after">after</value>
							<value id="both">both</value>
						</values>
					</parameter>
				</parameters>
			</decorator>
		</object>
	
		<object id="\t41\View\TemplateComponent">
			<decorator id="PdfDefault" extends="\t41\View\Decorator\AbtractPdfDecorator">
				<parameters>
					<parameter id="newpage">
						<type>string</type>
						<defaultvalue>no</defaultvalue>
						<values>
							<value id="no">no</value>
							<value id="before">before</value>
							<value id="after">after</value>
							<value id="both">both</value>
						</values>
					</parameter>
				</parameters>
			</decorator>
		</object>
			
		<object id="\t41\View\ListComponent">
			<decorator id="WebDefault" extends="\t41\View\Decorator\AbtractWebDecorator">
				<parameters>
					<parameter id="paginator">
						<type>boolean</type>
						<defaultvalue>true</defaultvalue>
					</parameter>
					<parameter id="sortable">
						<type>boolean</type>
					<defaultvalue>true</defaultvalue>
					</parameter>
				</parameters>
			</decorator>
			<decorator id="PdfDefault" extends="\t41\View\Decorator\AbtractPdfDecorator">
				<parameters>
					<parameter id="newpage">
						<type>string</type>
						<defaultvalue>no</defaultvalue>
						<values>
							<value id="no">no</value>
							<value id="before">before</value>
							<value id="after">after</value>
							<value id="both">both</value>
						</values>
					</parameter>
				</parameters>
			</decorator>
		</object>
				
		<object id="\t41\View\FormComponent\Element\ButtonElement">
			<decorator id="WebDefault" extends="\t41\View\Decorator\AbtractWebDecorator">
				<parameters>
					<parameter id="size">
						<type>string</type>
						<defaultvalue>medium</defaultvalue>
					</parameter>
					<parameter id="icon">
						<type>string</type>
					</parameter>
					<parameter id="nolabel">
						<type>boolean</type>
						<defaultvalue>false</defaultvalue>
					</parameter>
				</parameters>
		</decorator>
		</object>
		
		<object id="\t41\View\FormComponent\Element\FieldElement">
			<decorator id="WebDefault" extends="\t41\View\Decorator\AbtractWebDecorator">
				<parameters>
					<parameter id="mode">
						<type>string</type>
					</parameter>
					<parameter id="length">
						<type>integer</type>
						<defaultvalue>30</defaultvalue>
					</parameter>
				</parameters>
		</decorator>
		</object>

		<object id="\t41\View\FormComponent\Element\ListElement">
			<decorator id="WebDefault" extends="\t41\View\Decorator\AbtractWebDecorator">
				<parameters>
					<parameter id="name">
						<type>string</type>
					</parameter>
					<parameter id="mode">
						<type>string</type>
					</parameter>
					<parameter id="defaultlabel">
						<type>string</type>
					</parameter>
					<parameter id="selectmax">
						<type>integer</type>
						<defaultvalue>30</defaultvalue>
					</parameter>
				</parameters>
			</decorator>
			<decorator id="WebAutocomplete" extends="\t41\View\Decorator\AbtractWebDecorator">
				<parameters>
					<parameter id="name">
						<type>string</type>
					</parameter>
					<parameter id="mode">
						<type>string</type>
					</parameter>
					<parameter id="searchprops">
						<type>string</type>
					</parameter>
					<parameter id="searchmode">
						<type>string</type>
						<defaultvalue>contains</defaultvalue>
					</parameter>
				</parameters>
			</decorator>
		</object>
		
		<object id="\t41\View\FormComponent\Element\EnumElement">
			<decorator id="WebDefault" extends="\t41\View\Decorator\AbtractWebDecorator">
				<parameters>
					<parameter id="mode">
						<type>string</type>
					</parameter>
					<parameter id="radiomax">
						<type>integer</type>
						<defaultvalue>3</defaultvalue>
					</parameter>
					<parameter id="defaultlabel">
						<type>string</type>
					</parameter>
				</parameters>
		</decorator>
		</object>
				
	</decorators>
</config>
