<?xml version="1.0" encoding="UTF-8"?>
<messages>
	<exceptions>
		<exception id="PROPERTY_INSTANCIATION_ERROR">
			<label lang="en">Error instanciating t41_Property object</label>
		</exception>
		<exception id="VALUE_MUST_BE_INSTANCEOF">
			<label lang="en">Value must be an instance of class '%0', 't41_Data_Object' or 't41_Object_Uri'. Given value was of type '%1'.</label>
		</exception>
		<exception id="VALUE_NOT_IN_ENUMERATION">
			<label lang="en">Value '%0' is not part of the acceptable values set (%1)</label>
		</exception>
		<exception id="VALUE_NOT_A_DATE">
			<label lang="en">Given value '%1' for '%0' is not a valid date</label>
		</exception>
		<exception id="VALUE_NOT_INTEGER_OR_FLOAT">
			<label lang="en">Given value '%0' is not an integer or a float</label>
		</exception>
		<exception id="VALUE_PROTECTED_FROM_CHANGE">
			<label lang="en">Value is protected and cannot be modified</label>
		</exception>
		<exception id="VALUE_VALIDATOR_DATE_FAILED">
			<label lang="en">Invalid Date: '%0'</label>
		</exception>
		<exception id="VALUE_VALIDATOR_EMAILADDRESS_FAILED">
			<label lang="en">Invalid Email Address Syntax: '%0'</label>
		</exception>
	</exceptions>
</messages>